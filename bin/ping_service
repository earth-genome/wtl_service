#!/usr/bin/python3

import requests

if __name__ == '__main__':
   resp = requests.get('http://earthrise-wtl.herokuapp.com/retrieve?daysback=1')
   print('Pinged wtl-service with status code: {}'.format(resp.status_code))
   if resp.status_code == 200:
       print('In the past 24 hours {} stories '.format(len(resp.json())) +
             'have been posted to WTL.')
